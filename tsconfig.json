{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "bake-app/schemas/documents/product.tsx",
    "bake-app/schemas/documents/collection.tsx",
    "bake-app/schemas/documents/colorTheme.tsx",
    "sanity.js",
    "sanity/schemas/category.js",
    "sanity/schemas/index.js",
    "sanity/schemas/product.js"
, "pages/api/checkout_session.tsx"  ],
  "exclude": ["node_modules"]
}
